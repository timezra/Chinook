<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width" initial-scale="1.0" />
    <meta name="description" content="ChinookUI description" />
    <meta name="author" content="timezra" />
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="/Content/styles/app.css" type="text/css" />
    @RenderSection("Styles", false)

    @if (Request.Browser.Browser == "IE" && Request.Browser.MajorVersion <= 9)
    {
      if (Request.Browser.MajorVersion <= 7)
      {
        @:<script src="http://cdnjs.cloudflare.com/ajax/libs/json2/20110223/json2.min.js" type="text/javascript"></script>
      }
      @:<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.min.js" type="text/javascript"></script>
      @:<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js" type="text/javascript"></script>
    }
    <script type="text/javascript">
      (window.brunch || {})["auto-reload"] = { enabled: true };
    </script>
    @RenderSection("Scripts", false)
</head>
<body>
	@RenderBody()
</body>
</html>

